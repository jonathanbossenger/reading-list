{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "reading-list/reading-list",
	"version": "1.0.0",
	"title": "Reading List",
	"category": "widgets",
	"icon": "book",
	"description": "Display your reading list of books.",
	"supports": {
		"html": false,
		"align": ["wide", "full"]
	},
	"textdomain": "reading-list",
	"attributes": {
		"limit": {
			"type": "number",
			"default": -1
		},
		"orderby": {
			"type": "string",
			"default": "date_read"
		},
		"order": {
			"type": "string",
			"default": "DESC"
		}
	},
	"editorScript": "file:./index.js",
	"render": "file:./render.php"
}
